		CalculatorStack cs = new CalculatorStack();
		
		StackComputing stc = new StackComputing(cs);
		
		Scanner sc = new Scanner(System.in);
		
		while (!sc.next().equalsIgnoreCase("quit")) {
			String inData = sc.next();
			
			switch (inData) {
			case "*":
				
			}
		}
	}
}
