package calculator;

public class StackComputing {
	CalculatorStack cs;
	
	public StackComputing(CalculatorStack cs) {
		this.cs = cs;
	}
	
	private int plus() {
		int temp = cs.pop() + cs.pop();
		
		return cs.push(temp);
	}
	
	private int minus() {
		int temp = cs.pop() - cs.pop();
		
		return cs.push(temp);
	}
	
	private int times() {
		int temp = cs.pop() * cs.pop();
		
		return cs.push(temp);
	}
	
	private int divides() {
		int temp = cs.pop() / cs.pop();
		
		return cs.push(temp);
	}
}
