package calculator;

public class StackComputing {
	CalculatorStack cs;
	
	public StackComputing(CalculatorStack cs) {
		this.cs = cs;
	}
	
	/**
	 * This will add the two top elements in the CalculatorStack.
	 * @return the calculated value
	 */
	public int add() {
		int temp = cs.pop() + cs.pop();
		
		return cs.push(temp);
	}
	
}
